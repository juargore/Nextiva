<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="@color/connectWhite" />
            <stroke android:width="4dp" android:color="@color/transparent"/>
            <size android:width="@dimen/general_view_medium" android:height="@dimen/general_view_medium" />
        </shape>
    </item>
    <item android:state_checked="true" android:state_enabled="true">
        <shape android:shape="oval">
            <solid android:color="@color/connectWhite" />
            <stroke android:width="4dp" android:color="@color/transparent"/>
            <size android:width="@dimen/general_view_medium" android:height="@dimen/general_view_medium" />
        </shape>
    </item>
    <item android:state_checked="true" android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="@color/connectWhite" />
            <stroke android:width="4dp" android:color="@color/transparent"/>
            <size android:width="@dimen/general_view_medium" android:height="@dimen/general_view_medium" />
        </shape>
    </item>
</selector>