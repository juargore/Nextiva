<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/list_item_nextiva_anywhere_location_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/selector_nextiva_orange_30">

    <com.nextiva.nextivaapp.android.view.DetailItemView
        android:id="@+id/list_item_nextiva_anywhere_location_detail_item_view"
        style="@style/DetailItemViewLayoutStyle"
        android:layout_width="match_parent"
        android:layout_toStartOf="@+id/list_item_nextiva_anywhere_location_enabled_text_view"
        android:background="@drawable/selector_nextiva_orange_30" />

    <TextView
        android:id="@+id/list_item_nextiva_anywhere_location_enabled_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="@dimen/general_horizontal_margin_medium" />
</RelativeLayout>